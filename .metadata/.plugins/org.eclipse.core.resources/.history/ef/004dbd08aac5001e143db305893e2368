#include <stdint.h>

#include <stm32f4xx.h>  // Library for STM32f407

#define GPIOAEN          (1U<<0)  //TO enable GPIOA
#define ADC1EN           (1U<<8)  //TO enable ADC1


void ADC_Init (void){

	RCC -> AHB1ENR |= GPIOAEN;  // Enable clock to GPIOA
	GPIOA -> MODDER |= (1<<2);  //
	GPIOA -> MODDER |= (1<<3);


}

int main(){

}
